{
    "manifest_version": 3,
    "name": "Cookie快取",
    "version": "2.0",
    "description": "显示当前站点的cookie、域名和UA信息",
    "permissions": [
      "cookies", 
      "tabs", 
      "activeTab",
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }